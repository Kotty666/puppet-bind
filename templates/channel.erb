  channel <%= @channel -%> {
<% if !@filename.empty? -%>
    filename <%= @filename -%>
<% end -%>  
<% if !@fileversion.empty? -%>
 <%= @fileversion -%>
<% end -%>  
<% if !@filesize.empty? -%>
 <%= @filesize -%>
<% end -%>
